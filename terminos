#include <iostream>

using namespace std;
int multent(int n);

int main()
{
    int n,x,i;
    
    cout<<"Ingrese el numero de terminos: ";
    cin>>n;
    x= multent(n);
    cout<<"El resultado de la multiplicacion es: "<<x<<endl;

    return 0;
}
    int multent(int n){
        int m=1;
        
        for(int i=1;i<=n;i++){
           m= m*i;
            
        }
        return m;
    }
