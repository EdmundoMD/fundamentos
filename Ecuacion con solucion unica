#include <iostream>
#include <cmath>
using namespace std;

int main()
{
float A,B,C,D,x;
cout<<"Ingrese el valor de A: ";
cin>>A; 

cout<<"Ingrese el valor B: ";
cin>>B;

cout<<"Ingrese el valor C: ";
cin>>C;

D= B*B-(4*A*C);

x=-B/(2*A);

if(D=0){x;
}

cout<<"El valor de x es:"<<x;

    return 0;
}
