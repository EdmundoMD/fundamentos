#include <iostream>

using namespace std;

int main()
{
    float L,area,p;
        cout<<"Ingrese lado: ";
        cin>>L;
        
       area=L*L;
       p=L+L+L+L;
    
    cout<<"El area del cuadrado es: "<<area<<endl;
    cout<<"El perimetro del cuadrado es: "<<p;
      
    

    return 0;
}
